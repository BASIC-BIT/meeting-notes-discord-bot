{
  "name": "meeting-minutes-discord-bot",
  "version": "1.0.0",
  "description": "Record and transcribe conversations in Discord",
  "main": "src/index.ts",
  "scripts": {
    "start": "nodemon --exec ts-node src/index.ts",
    "start:mock": "nodemon --exec \"ts-node src/index.ts --mock\"",
    "start:mock:once": "ts-node src/index.ts --mock",
    "build": "tsc",
    "build:web": "vite build",
    "build:all": "yarn build && yarn build:web",
    "serve": "node dist/index.js",
    "terraform:init": "terraform -chdir=_infra init",
    "terraform:plan": "terraform -chdir=_infra plan",
    "terraform:apply": "terraform -chdir=_infra apply",
    "terraform:plan:prod": "terraform -chdir=_infra plan -var-file=terraform.tfvars",
    "terraform:apply:prod": "terraform -chdir=_infra apply -var-file=terraform.tfvars",
    "terraform:plan:staging": "terraform -chdir=_infra plan -var-file=terraform.staging.tfvars",
    "terraform:apply:staging": "terraform -chdir=_infra apply -var-file=terraform.staging.tfvars",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint --fix src test",
    "lint:check": "eslint src test",
    "prettier": "prettier . --write",
    "prettier:check": "prettier . --check",
    "frontend:dev": "vite",
    "frontend:preview": "vite preview",
    "prepare": "husky",
    "init": "npm install --legacy-peer-deps && npx husky init && npm run init:husky-hooks",
    "init:husky-hooks": "echo 'npx lint-staged' > .husky/pre-commit",
    "docker:setup": "node scripts/setup-dynamodb-dir.js",
    "docker:up": "npm run docker:setup && docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "node scripts/clean-dynamodb.js",
    "db:init": "node scripts/init-dynamodb.js",
    "dev": "npm run docker:up && npm run db:init && npm run start",
    "dev:mock": "npm run docker:up && npm run db:init && nodemon --exec \"ts-node src/index.ts --mock\"",
    "dev:clean": "npm run docker:down && npm run docker:clean && npm run dev",
    "observability:up": "docker compose -f docker-compose.observability.yml up -d",
    "observability:down": "docker compose -f docker-compose.observability.yml down",
    "docker:minio-init": "docker-compose run --rm minio-init",
    "checkov": "uvx --from checkov checkov -d _infra --quiet --compact",
    "check": "yarn lint && yarn prettier && yarn test && yarn build:all && yarn code:stats",
    "check:ci": "yarn lint:check && yarn prettier:check && yarn test && yarn build && yarn code:stats",
    "stripe:listen": "stripe listen --forward-to http://localhost:3001/api/billing/webhook",
    "stripe:trigger": "stripe trigger",
    "stripe:trigger:checkout": "stripe trigger checkout.session.completed",
    "stripe:trigger:invoice-paid": "stripe trigger invoice.payment_succeeded",
    "stripe:trigger:invoice-failed": "stripe trigger invoice.payment_failed",
    "stripe:trigger:subscription-updated": "stripe trigger customer.subscription.updated",
    "stripe:trigger:subscription-deleted": "stripe trigger customer.subscription.deleted",
    "code:stats": "scc --ci && scc --ci --by-file --sort lines && python -m lizard -l ts -l tsx -l js -l jsx src test"
  },
  "engines": {
    "node": "~20.19.2"
  },
  "author": "BASIC-BIT",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.956.0",
    "@aws-sdk/client-s3": "^3.956.0",
    "@aws-sdk/s3-request-presigner": "^3.956.0",
    "@aws-sdk/util-dynamodb": "^3.956.0",
    "@discordjs/opus": "^0.10.0",
    "@discordjs/rest": "^2.5.1",
    "@discordjs/voice": "^0.18.0",
    "@mantine/core": "^8.3.0",
    "@mantine/hooks": "^8.3.0",
    "@mantine/notifications": "^8.3.0",
    "@tabler/icons-react": "^3.36.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-router": "^1.141.8",
    "@trpc/client": "^11.8.0",
    "@trpc/react-query": "^11.8.0",
    "@trpc/server": "^11.8.0",
    "aws-sdk": "^2.1640.0",
    "bottleneck": "^2.19.5",
    "cockatiel": "^3.2.1",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "diff": "^5.2.0",
    "discord-api-types": "^0.38.12",
    "discord.js": "^14.16.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "fastest-levenshtein": "^1.0.16",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "libsodium-wrappers": "^0.7.15",
    "nodemon": "^3.1.10",
    "openai": "^5.5.1",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "prism-media": "^1.3.5",
    "prom-client": "15.1.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "stripe": "^14.24.0",
    "uuid": "^9.0.0",
    "web-vitals": "^4.2.4",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.17",
    "@types/diff": "^5.0.9",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.17.10",
    "@types/passport": "^1.0.17",
    "@types/passport-discord": "^0.1.14",
    "@types/react": "^19.0.3",
    "@types/react-dom": "^19.0.2",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "babel-jest": "^29.7.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.3",
    "yarn": "^1.22.22"
  },
  "funding": "https://ko-fi.com/basicbit",
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
