{
  "name": "meeting-minutes-discord-bot",
  "version": "1.0.0",
  "description": "Record and transcribe conversations in Discord",
  "main": "src/index.ts",
  "scripts": {
    "start": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "serve": "node dist/index.js",
    "terraform:init": "terraform -chdir=_infra init",
    "terraform:plan": "terraform -chdir=_infra plan",
    "terraform:apply": "terraform -chdir=_infra apply",
    "test": "jest",
    "lint": "eslint --fix src test",
    "prettier": "prettier . --write",
    "frontend:start": "react-scripts start",
    "frontend:build": "react-scripts build",
    "frontend:test": "react-scripts test",
    "frontend:eject": "react-scripts eject",
    "prepare": "husky",
    "init": "npm install --legacy-peer-deps && npx husky init && npm run init:husky-hooks",
    "init:husky-hooks": "echo 'npx lint-staged' > .husky/pre-commit",
    "docker:setup": "node scripts/setup-dynamodb-dir.js",
    "docker:up": "npm run docker:setup && docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "node scripts/clean-dynamodb.js",
    "db:init": "node scripts/init-dynamodb.js",
    "dev": "npm run docker:up && npm run db:init && npm run start",
    "dev:clean": "npm run docker:down && npm run docker:clean && npm run dev"
  },
  "engines": {
    "node": "~20.19.2"
  },
  "author": "BASIC-BIT",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.830.0",
    "@aws-sdk/util-dynamodb": "^3.830.0",
    "@discordjs/opus": "^0.10.0",
    "@discordjs/rest": "^2.5.1",
    "@discordjs/voice": "^0.18.0",
    "bottleneck": "^2.19.5",
    "cockatiel": "^3.2.1",
    "date-fns": "^3.6.0",
    "diff": "^5.2.0",
    "discord-api-types": "^0.38.12",
    "discord.js": "^14.16.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "fastest-levenshtein": "^1.0.16",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "libsodium-wrappers": "^0.7.15",
    "nodemon": "^3.1.10",
    "openai": "^5.5.1",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "prism-media": "^1.3.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.11.1",
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/diff": "^5.0.9",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jest": "^27.5.2",
    "@types/node": "^20.17.10",
    "@types/passport": "^1.0.17",
    "@types/passport-discord": "^0.1.14",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "babel-jest": "^29.7.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.34.1",
    "yarn": "^1.22.22"
  },
  "funding": "https://ko-fi.com/basicbit",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
