{
  "title": "Meeting Notes Bot - Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "type": "stat",
      "title": "Meeting Starts / min",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource_uid}" },
      "targets": [
        {
          "expr": "rate(meeting_started_total[5m])",
          "legendFormat": "starts/min",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Live Voice Responses / min",
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource_uid}" },
      "targets": [
        {
          "expr": "rate(live_voice_responses_total[5m])",
          "legendFormat": "responses/min",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP p95 latency (s)",
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${datasource_uid}" },
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ]
    },
    {
      "type": "bargauge",
      "title": "HTTP error rate (5xx)",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 12 },
      "datasource": { "type": "prometheus", "uid": "${datasource_uid}" },
      "fieldConfig": {
        "defaults": { "unit": "ops" },
        "overrides": []
      },
      "options": { "displayMode": "basic" },
      "targets": [
        {
          "expr": "sum(rate(http_request_duration_seconds_count{status=~\"5..\"}[5m])) by (route)",
          "legendFormat": "{{route}}",
          "refId": "A"
        }
      ]
    }
  ]
}
